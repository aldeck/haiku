	NetworkApp.cpp
	NetworkWindow.cpp
	EthernetSettingsView.cpp
	Settings.cpp

	: be $(HAIKU_NETWORK_LIBS) $(TARGET_LIBSUPC++) $(HAIKU_LOCALE_LIBS)
